<a href="https://go.smzdm.com/f8e174ad4272b4e4/ca_bb_yc_163_70999971_11386_1933_175_0" onclick="gtmAddToCart({'name':'小米蓝牙耳机AirDots青春版','id':'a6lree8g' ,'dimension16':'6105415350','dimension19':'213070643315698274950835488d3248e904383d501d40dfe2d26f203f', 'price':'199','brand':'无' ,'mall':'小米官网', 'category':'无/无/无/无','metric1':'199','dimension10':'mi.com','dimension9':'yuanchuang','dimension11':'4阶价格','dimension20':'无','dimension25':'无','dimension64':'yuanchuang'});if(typeof change_direct_url != 'undefined' && change_direct_url instanceof Function){change_direct_url(this)}" target="_blank" rel="nofollow" >
